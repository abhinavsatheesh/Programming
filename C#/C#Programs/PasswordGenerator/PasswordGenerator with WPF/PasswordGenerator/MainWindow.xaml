<Window x:Class="PasswordGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordGenerator"
        mc:Ignorable="d"
        Title="Password Generator" Height="450" Width="800"
        Loaded="Window1_Load">
    <Grid>
        <TextBlock x:Name="label1" HorizontalAlignment="Left" Margin="350,10,0,0" TextWrapping="Wrap" Text="Password Generator" VerticalAlignment="Top"/>
        <TextBlock x:Name="label2" HorizontalAlignment="Left" Margin="318,50,0,0" TextWrapping="Wrap" Text="Hi. Welcome to Password Generator" VerticalAlignment="Top" Height="19" Width="193"/>
        <TextBlock x:Name="label3" HorizontalAlignment="Left" Margin="314,74,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <Button Content="Generate another Password?" HorizontalAlignment="Left" Margin="333,115,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Copy Password" HorizontalAlignment="Left" Margin="576,74,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
